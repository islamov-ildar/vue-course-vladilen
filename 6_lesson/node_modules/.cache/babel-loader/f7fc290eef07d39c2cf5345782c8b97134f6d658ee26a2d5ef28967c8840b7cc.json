{"ast":null,"code":"import AppBtn from \"./AppBtn.vue\";\nimport AppTextOne from \"./AppTextOne.vue\";\nimport AppTextTwo from \"./AppTextTwo.vue\";\nexport default {\n  name: \"App\",\n  data: function data() {\n    return {\n      active: \"one\"\n    };\n  },\n  components: {\n    AppBtn: AppBtn,\n    AppTextOne: AppTextOne,\n    AppTextTwo: AppTextTwo\n  },\n  computed: {\n    componentName: function componentName() {\n      // return this.active === \"one\" ? \"AppTextOne\" : \"AppTextTwo\";\n      return \"app-text-\" + this.active;\n    },\n    oneColor: function oneColor() {\n      return this.active === \"one\" ? \"primary\" : \"\";\n    },\n    twoColor: function twoColor() {\n      return this.active === \"two\" ? \"primary\" : \"\";\n    }\n  }\n};","map":{"version":3,"names":["AppBtn","AppTextOne","AppTextTwo","name","data","active","components","computed","componentName","oneColor","twoColor"],"sources":["D:\\Projects\\vue3-course\\6_lesson\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"container pt-1\">\r\n    <div class=\"card\">\r\n      <h1>Dynamic and asynchronous components</h1>\r\n      <app-btn @action=\"active = 'one'\" :color=\"oneColor\">one</app-btn>\r\n      <app-btn @action=\"active = 'two'\" :color=\"twoColor\">two</app-btn>\r\n    </div>\r\n    <!-- <AppTextOne v-if=\"active === 'one'\" />\r\n    <AppTextTwo v-else /> -->\r\n    <keep-alive>\r\n      <component :is=\"componentName\"></component>\r\n    </keep-alive>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppBtn from \"./AppBtn.vue\";\r\nimport AppTextOne from \"./AppTextOne.vue\";\r\nimport AppTextTwo from \"./AppTextTwo.vue\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n  data() {\r\n    return {\r\n      active: \"one\",\r\n    };\r\n  },\r\n  components: { AppBtn, AppTextOne, AppTextTwo },\r\n  computed: {\r\n    componentName() {\r\n      // return this.active === \"one\" ? \"AppTextOne\" : \"AppTextTwo\";\r\n      return \"app-text-\" + this.active;\r\n    },\r\n    oneColor() {\r\n      return this.active === \"one\" ? \"primary\" : \"\";\r\n    },\r\n    twoColor() {\r\n      return this.active === \"two\" ? \"primary\" : \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAgBA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,UAAS,MAAO,kBAAkB;AACzC,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IAAEN,MAAM,EAANA,MAAM;IAAEC,UAAU,EAAVA,UAAU;IAAEC,UAAS,EAATA;EAAW,CAAC;EAC9CK,QAAQ,EAAE;IACRC,aAAa,WAAAA,cAAA,EAAG;MACd;MACA,OAAO,WAAU,GAAI,IAAI,CAACH,MAAM;IAClC,CAAC;IACDI,QAAQ,WAAAA,SAAA,EAAG;MACT,OAAO,IAAI,CAACJ,MAAK,KAAM,KAAI,GAAI,SAAQ,GAAI,EAAE;IAC/C,CAAC;IACDK,QAAQ,WAAAA,SAAA,EAAG;MACT,OAAO,IAAI,CAACL,MAAK,KAAM,KAAI,GAAI,SAAQ,GAAI,EAAE;IAC/C;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}