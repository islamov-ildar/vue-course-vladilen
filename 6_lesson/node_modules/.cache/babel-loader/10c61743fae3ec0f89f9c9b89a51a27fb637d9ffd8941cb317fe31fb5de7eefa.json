{"ast":null,"code":"import \"core-js/modules/web.timers.js\";\nimport AppBtn from \"./AppBtn.vue\";\nimport AppTextOne from \"./AppTextOne.vue\";\nimport AppTextTwo from \"./AppTextTwo.vue\";\nexport default {\n  name: \"App\",\n  data: function data() {\n    return {\n      active: \"one\"\n    };\n  },\n  components: {\n    AppBtn: AppBtn,\n    AppTextOne: AppTextOne,\n    AppTextTwo: AppTextTwo\n  },\n  mounted: function mounted() {\n    var _this = this;\n    setTimeout(function () {\n      _this.componentName = \"asd\";\n    }, 1500);\n    console.log(this.$refs.myBtn);\n  },\n  computed: {\n    componentName: {\n      // return this.active === \"one\" ? \"AppTextOne\" : \"AppTextTwo\";\n      // return \"app-text-\" + this.active;\n      get: function get() {\n        return \"app-text-\" + this.active;\n      },\n      set: function set(value) {\n        console.log(\"set\", value);\n      }\n    },\n    oneColor: function oneColor() {\n      return this.active === \"one\" ? \"primary\" : \"\";\n    },\n    twoColor: function twoColor() {\n      return this.active === \"two\" ? \"primary\" : \"\";\n    }\n  }\n};","map":{"version":3,"names":["AppBtn","AppTextOne","AppTextTwo","name","data","active","components","mounted","_this","setTimeout","componentName","console","log","$refs","myBtn","computed","get","set","value","oneColor","twoColor"],"sources":["D:\\Projects\\vue3-course\\6_lesson\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"container pt-1\">\r\n    <div class=\"card\">\r\n      <h1>Dynamic and asynchronous components</h1>\r\n      <app-btn ref=\"myBtn\" @action=\"active = 'one'\" :color=\"oneColor\"\r\n        >one</app-btn\r\n      >\r\n      <app-btn @action=\"active = 'two'\" :color=\"twoColor\">two</app-btn>\r\n    </div>\r\n    <!-- <AppTextOne v-if=\"active === 'one'\" />\r\n    <AppTextTwo v-else /> -->\r\n    <keep-alive>\r\n      <component :is=\"componentName\"></component>\r\n    </keep-alive>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppBtn from \"./AppBtn.vue\";\r\nimport AppTextOne from \"./AppTextOne.vue\";\r\nimport AppTextTwo from \"./AppTextTwo.vue\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n  data() {\r\n    return {\r\n      active: \"one\",\r\n    };\r\n  },\r\n  components: { AppBtn, AppTextOne, AppTextTwo },\r\n  mounted() {\r\n    setTimeout(() => {\r\n      this.componentName = \"asd\";\r\n    }, 1500);\r\n\r\n    console.log(this.$refs.myBtn);\r\n  },\r\n  computed: {\r\n    componentName: {\r\n      // return this.active === \"one\" ? \"AppTextOne\" : \"AppTextTwo\";\r\n      // return \"app-text-\" + this.active;\r\n      get() {\r\n        return \"app-text-\" + this.active;\r\n      },\r\n      set(value) {\r\n        console.log(\"set\", value);\r\n      },\r\n    },\r\n    oneColor() {\r\n      return this.active === \"one\" ? \"primary\" : \"\";\r\n    },\r\n    twoColor() {\r\n      return this.active === \"two\" ? \"primary\" : \"\";\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":";AAkBA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,UAAS,MAAO,kBAAkB;AACzC,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IAAEN,MAAM,EAANA,MAAM;IAAEC,UAAU,EAAVA,UAAU;IAAEC,UAAS,EAATA;EAAW,CAAC;EAC9CK,OAAO,WAAAA,QAAA,EAAG;IAAA,IAAAC,KAAA;IACRC,UAAU,CAAC,YAAM;MACfD,KAAI,CAACE,aAAY,GAAI,KAAK;IAC5B,CAAC,EAAE,IAAI,CAAC;IAERC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,KAAK,CAACC,KAAK,CAAC;EAC/B,CAAC;EACDC,QAAQ,EAAE;IACRL,aAAa,EAAE;MACb;MACA;MACAM,GAAG,WAAAA,IAAA,EAAG;QACJ,OAAO,WAAU,GAAI,IAAI,CAACX,MAAM;MAClC,CAAC;MACDY,GAAG,WAAAA,IAACC,KAAK,EAAE;QACTP,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEM,KAAK,CAAC;MAC3B;IACF,CAAC;IACDC,QAAQ,WAAAA,SAAA,EAAG;MACT,OAAO,IAAI,CAACd,MAAK,KAAM,KAAI,GAAI,SAAQ,GAAI,EAAE;IAC/C,CAAC;IACDe,QAAQ,WAAAA,SAAA,EAAG;MACT,OAAO,IAAI,CAACf,MAAK,KAAM,KAAI,GAAI,SAAQ,GAAI,EAAE;IAC/C;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}