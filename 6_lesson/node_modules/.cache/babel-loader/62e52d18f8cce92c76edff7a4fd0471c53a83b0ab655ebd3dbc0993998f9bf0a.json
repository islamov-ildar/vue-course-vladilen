{"ast":null,"code":"import AppBtn from \"./AppBtn.vue\";\nimport AppTextOne from \"./AppTextOne.vue\";\nimport AppTextTwo from \"./AppTextTwo.vue\";\nexport default {\n  name: \"App\",\n  data: function data() {\n    return {\n      active: \"one\"\n    };\n  },\n  components: {\n    AppBtn: AppBtn,\n    AppTextOne: AppTextOne,\n    AppTextTwo: AppTextTwo\n  }\n};","map":{"version":3,"names":["AppBtn","AppTextOne","AppTextTwo","name","data","active","components"],"sources":["D:\\Projects\\vue3-course\\6_lesson\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"container pt-1\">\r\n    <div class=\"card\">\r\n      <h1>Dynamic and asynchronous components</h1>\r\n      <app-btn\r\n        @action=\"active = 'one'\"\r\n        :color=\"active === 'one' ? 'primary' : ''\"\r\n        >one</app-btn\r\n      >\r\n      <app-btn\r\n        @action=\"active = 'two'\"\r\n        :color=\"active === 'two' ? 'primary' : ''\"\r\n        >two</app-btn\r\n      >\r\n    </div>\r\n    <AppTextOne v-if=\"active === 'one'\" />\r\n    <AppTextTwo v-else />\r\n\r\n    <component :is=\"'AppTextOne'\"></component>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AppBtn from \"./AppBtn.vue\";\r\nimport AppTextOne from \"./AppTextOne.vue\";\r\nimport AppTextTwo from \"./AppTextTwo.vue\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n  data() {\r\n    return {\r\n      active: \"one\",\r\n    };\r\n  },\r\n  components: { AppBtn, AppTextOne, AppTextTwo },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAuBA,OAAOA,MAAK,MAAO,cAAc;AACjC,OAAOC,UAAS,MAAO,kBAAkB;AACzC,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;IACV,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IAAEN,MAAM,EAANA,MAAM;IAAEC,UAAU,EAAVA,UAAU;IAAEC,UAAS,EAATA;EAAW;AAC/C,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}